}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
    ralt:                               '\u00df'
    ralt+shift:                         '\u1e9e'
}

key MINUS {
    label:                              '-'
    base:                               '-'
    shift, capslock:                    '_'
    ralt:                               '\u00ad'
    ralt+shift:                         '\u2013'
}

key BACKSLASH {
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
}

### ROW 4

key PLUS {
    label:                              '<'
    base:                               '<'
    shift:                              '>'
    ralt:                               '|'
    ralt+shift:                         '\u00a6'
}

key APOSTROPHE {
    label:                              '\''
